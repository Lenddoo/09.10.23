programa {
  funcao inicio() {
    
    
    inteiro cont=1 ,contador = 1 
    inteiro tab=0

    escreva("Informe a tabuada desejada: ")
      leia(tab)
    enquanto(contador <= 10){
      escreva ("\n",tab," x ",cont,": ",tab*contador)
        contador = contador + 1
        cont = cont + 1
    }
    
  }
}
